rI","executionInfo":{"status":"ok","timestamp":1594225319757,"user_tz":300,"elapsed":396,"user":{"displayName":"Gerardo Mauricio Toledo Acosta","photoUrl":"https://lh3.googleusercontent.com/a-/AOh14GjpdcJ_OCkuYV3gvXX_QYOg4_5A7L5rDcknBW2V=s64","userId":"07874452728097482203"}},"outputId":"71b21f72-cf96-4af1-c0e1-768684ec180a","colab":{"base_uri":"https://localhost:8080/","height":35}},"source":["total_idxs_to_go = available_df.loc[available_df['Trs'] != True].index.to_list()\n","reverse_total_idxs_to_go = total_idxs_to_go.copy()\n","reverse_total_idxs_to_go.reverse()\n","index_to_do = reverse_total_idxs_to_go[:36]\n","print(index_to_do[:5])"],"execution_count":null,"outputs":[{"output_type":"stream","text":["[1884, 1881, 1878, 1113, 1112]\n"],"name":"stdout"}]},{"cell_type":"code","metadata":{"id":"3eBPWlknSxVK"},"source":["total_count = 0\n","total_to_reach = len(index_to_do)"],"execution_count":null,"outputs":[]},{"cell_type":"code","metadata":{"id":"OPISGgoTRm29","executionInfo":{"status":"ok","timestamp":1594231454676,"user_tz":300,"elapsed":556,"user":{"displayName":"Gerardo Mauricio Toledo Acosta","photoUrl":"https://lh3.googleusercontent.com/a-/AOh14GjpdcJ_OCkuYV3gvXX_QYOg4_5A7L5rDcknBW2V=s64","userId":"07874452728097482203"}},"outputId":"ab019106-abc6-4015-95d4-3c714414855a","colab":{"base_uri":"https://localhost:8080/","height":106}},"source":["img_path = '/content/drive/My Drive/Diplomado Python/Notebooks & Resources/Proyecto Final/Images-transposed/'\n","\n","much_intervals = [k for k in range(-7,8)]\n","much_intervals.remove(0)\n","much_intervals.remove(-1)\n","much_intervals.remove(1)\n","total = len(index_to_do)\n","fig = plt.figure(figsize=(7, 5),frameon=False)\n","ax = fig.add_subplot(111)\n","count = 0\n","how_many = 20\n","print(\"Van\",total_count,\"de\",total)\n","for k in index_to_do:\n","    if count<how_many:\n","        done = available_df.loc[k,'Trs']\n","        if done != True:\n","            midi_path = available_df.loc[k,'Midi_Path']\n",